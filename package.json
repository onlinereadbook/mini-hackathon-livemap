{
  "name": "webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "6.9.1"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "DEV=true nodemon ./dist/app.js --exec babel-node",
    "build-frontend": "webpack -p --config webpack-deploy.config.js --progress --colors",
    "build-backend": "babel ./server -s --presets=latest --plugins=transform-runtime -d ./dist",
    "start": "node ./dist/app.js",
    "server": "npm run build-frontend && npm run build-backend && npm start",
    "heroku": "npm run build-frontend && npm run build-backend && git add . && git push",
    "deploy": "npm run build-frontend && npm run build-backend && npm start",
    "dev:front": "webpack-dev-server --config webpack-dev-server.config.js --port 8888 --hot --devtool eval --progress --colors --content-base app ",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test:server": "Unitest=true mocha --compilers js:babel-core/register"
  },
  "keywords": [],
  "author": "mini-hackathon",
